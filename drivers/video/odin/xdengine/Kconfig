menuconfig ODIN_XDENGINE
        tristate "ODIN XD Engine support"
        depends on ODIN_DSS
        help
	  S3D driver for ODIN based boards.

config ODIN_XDENGINE_DEBUG_SUPPORT
        bool "Debug support for ODIN XD Engine"
	default y
	depends on ODIN_XDENGINE
	help
	  Support for debug output. You have to enable the actual printing
	  with 'debug' module parameter.
