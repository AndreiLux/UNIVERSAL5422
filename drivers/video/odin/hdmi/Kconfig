menuconfig ODIN_HDMI
        tristate "ODIN HDMI support"
        depends on ODIN_DSS
        help
	  HDMI driver for ODIN based boards.

config ODIN_HDMI_DEBUG_SUPPORT
        bool "Debug support for ODIN HDMI"
	default y
	depends on ODIN_HDMI
	help
	  Support for debug output. You have to enable the actual printing
	  with 'debug' module parameter.
