menuconfig ODIN_DSSCOMP
        tristate "ODIN DSS Composition Support"
        depends on ARCH_ODIN
        help
	  display subsystem driver for ODIN based boards.

if ODIN_DSSCOMP
config DSSCOMP_DEBUG_LOG
        bool "Debug support for ODIN DSS COMPOSITION"
	default y
	depends on ODIN_DSSCOMP
	help
	  Support for debug output. You have to enable the actual printing
	  with 'debug' module parameter.

endif
