				regulator-dsm-mode = <0x04>;
			};

			D2260_LDO_15 {
				regulator-name = "+3.0V_VDD_uSD";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
				/*
				LGE_CHANGE_S
				*/
				regulator-always-on;
				/* LGE_CHANGE_E */
			};

			D2260_LDO_16 {
				regulator-name = "+3.0V_SENSOR_AVDD";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
/* LGE_CHANGE_S
*/
				regulator-always-on;
/* LGE_CHANGE_E */
			};

			D2260_LDO_17 {
				regulator-name = "+1.2V_VT_CAM_DVDD";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
			};

			D2260_LDO_18 {
				regulator-name = "+1.8V_13M_VCM";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
			};

			D2260_LDO_19 {
				regulator-name = "+3.0V_VDD_MOTOR";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
			};

			D2260_LDO_20 {
				regulator-name = "+2.7V_13M_ANA";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
			};

			D2260_LDO_21 {
				regulator-name = "+1.8V_AVDD_DPHY_DSS_VCCA";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-dsm-mode = <0x04>;
			};

			D2260_LDO_22 {
				regulator-name = "+1.8V_TOUCH_IOVCC";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-dsm-mode = <0x06>;
			};

			D2260_LDO_23 {
				regulator-name = "+2.8V_VT_CAM_AVDD";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
