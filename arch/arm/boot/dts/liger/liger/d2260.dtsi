			};

			D2260_LDO_24 {
				regulator-name = "+1.05V_CAM_DVDD";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3100000>;
				regulator-dsm-mode = <0x00>;
			};

			D2260_LDO_25 {
				regulator-name = "+1.8V_VDD_CSS_MAIN_CAM_IO";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <3300000>;
				regulator-dsm-mode = <0x00>;
			};
		};
		gpios {
				compatible = "dlg,d2260-gpio";
				/* gpio num is auto generated.
				If this num is not working, please refer to the following statement.
				"cat /sys/kernel/debug/gpio"
				*/
				gpio@0{
					dlg,gpio_num = <510>; /* "nVDD_FAULT" */
				};
				gpio@1{
					dlg,gpio_num = <511>; /* "EXT_PWRON" */
				};
			};
		};
    };
};
