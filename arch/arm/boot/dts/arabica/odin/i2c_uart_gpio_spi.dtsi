			compatible = "LG,odin-gpio";
			reg = <0 0x330F0B00 0 0x100>;
			bank_num = <272>;
		};
	/*---- css gpio controller end ----*/

	};

	gpio-keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		autorepeat;
		button@1 {
			label = "KEY_VOLUMEDOWN";
			linux,code = <114>;
			gpios = <&gpio1 4 1>;
		};
		button@2 {
			label = "KEY_VOLUMEUP";
			linux,code = <115>;
			gpios = <&gpio1 5 1>;
		};
	};
};
