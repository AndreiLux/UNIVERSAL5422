/* i2c end */

/* spi start */
		spi@20011000 {
			compatible = "octasic,odin-spi";
			reg = <0 0x20011000 0 0x1000>;
			interrupts = <0 9 0x4>;
			clocks = <&spi_sclk0>;
			pclk_name = "spi_pclk0";
			enable_rpm = <1>;
			id = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			spidev@0{
				compatible = "spidev";
				spi-max-frequency = <1000000>;
				reg = <0>;
				spi-cpha;
				spi-cpol;
			};

			ethernet@1{
				compatible = "microschip,odin-ethernet";
				spi-max-frequency = <16000000>;
				spi_cs_gpio = <9>;
				reg = <3>;
				irq_gpio = <7>;
			};
		};

/* spi end */
/*---- peri gpio controller ----*/
/* G0_GPIO0~6 */
		gpio1: gpio-controller@20028000 {
			#gpio-cells = <2>;
			compatible = "LG,odin-gpio";
			reg = <0 0x20028000 0 0x800>;
			clocks = <&g0_gpio_pclk>;
			interrupts = <0 25 0x04>;
			bank_num = <0>;
		};
		gpio-controller@20028800 {
			compatible = "LG,odin-gpio";
			reg = <0 0x20028800 0 0x800>;
			clocks = <&g0_gpio_pclk>;
			bank_num = <8>;
		};
		gpio-controller@20029000 {
			compatible = "LG,odin-gpio";
			reg = <0 0x20029000 0 0x800>;
			clocks = <&g0_gpio_pclk>;
			bank_num = <16>;
		};
		gpio-controller@20029800 {
			compatible = "LG,odin-gpio";
			reg = <0 0x20029800 0 0x800>;
			clocks = <&g0_gpio_pclk>;
			bank_num = <24>;
		};
		gpio-controller@2002A000 {
			compatible = "LG,odin-gpio";
			reg = <0 0x2002A000 0 0x800>;
			clocks = <&g0_gpio_pclk>;
			bank_num = <32>;
		};
